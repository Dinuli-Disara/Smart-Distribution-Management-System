{
  "expo": {
    "name": "Manjula DMS",
    "slug": "manjula-dms",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.manjula.dms",
      "infoPlist": {
        "CFBundleURLTypes": [
          {
            "CFBundleURLSchemes": ["manjuladms"]
          }
        ]
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#1E3EA6"
      },
      "package": "com.manjula.dms",
      "intentFilters": [
        {
          "action": "VIEW",
          "data": [
            {
              "scheme": "manjuladms",
              "host": "reset"
            }
          ],
          "category": ["BROWSABLE", "DEFAULT"]
        }
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "scheme": "manjuladms",
    "extra": {
      "eas": {
        "projectId": "your-project-id"
      }
    }
  }
}